{
  "name": "professor-x",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && prettier . --check && stylelint **/*.scss && cspell **",
    "format": "eslint . --fix && prettier . --write && stylelint **/*.scss --fix",
    "eslint-check": "eslint .",
    "eslint-fix": "eslint . --fix",
    "code-format-check": "prettier . --check",
    "code-format": "prettier . --write",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "cspell-check": "cspell **",
    "preview": "vite preview",
    "test": "vitest",
    "test-coverage": "vitest run --coverage",
    "prepare-husky": "cd ../ && npx husky ./client/.husky",
    "lint-staged": "lint-staged",
    "postinstall": "npm run prepare-husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.3",
    "cspell": "^9.1.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-dom": "^1.52.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-react-x": "^1.52.2",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.1.2",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "prettier": "3.5.3",
    "sass-embedded": "^1.89.2",
    "stylelint": "^16.20.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
